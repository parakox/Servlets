<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>ParkingPlaceController</servlet-name>
        <servlet-class>controller.ParkingPlaceController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ParkingPlaceController</servlet-name>
        <url-pattern>/parking</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SignInSignUpController</servlet-name>
        <servlet-class>controller.SignInSignUpController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignInSignUpController</servlet-name>
        <url-pattern>/authentication</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AccountOfUserController</servlet-name>
        <servlet-class>controller.AccountOfUserController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccountOfUserController</servlet-name>
        <url-pattern>/account</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddCarController</servlet-name>
        <servlet-class>controller.AddCarController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddCarController</servlet-name>
        <url-pattern>/addCar</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DeleteCarController</servlet-name>
        <servlet-class>controller.DeleteCarController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteCarController</servlet-name>
        <url-pattern>/deleteCar</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>ParkCarController</servlet-name>
    <servlet-class>controller.ParkCarController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ParkCarController</servlet-name>
        <url-pattern>/parkCar</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UnparkCarController</servlet-name>
        <servlet-class>controller.UnparkCarController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UnparkCarController</servlet-name>
        <url-pattern>/unparkCar</url-pattern>
    </servlet-mapping>
</web-app>